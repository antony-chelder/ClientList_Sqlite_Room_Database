<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tony_clientlist.clientlist">
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_names"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.tony_clientlist.clientlist.MainActivity"

            android:label="@string/main_activiy"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tony_clientlist.clientlist.EditActivity"
                  android:label="@string/edit_activiy"

            />
        <activity android:name="com.tony_clientlist.clientlist.settings.SettingsActivity"
            android:label="@string/settings_activiy"

            />

    </application>


</manifest>